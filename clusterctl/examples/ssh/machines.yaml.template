items:
- apiVersion: "cluster.k8s.io/v1alpha1"
  kind: Machine
  metadata:
    generateName: ssh-controlplane-
    labels:
      set: controlplane
  spec:
    providerConfig:
      value:
        apiVersion: "sshproviderconfig/v1alpha1"
        kind: "SSHMachineProviderConfig"
        roles:
        - Master
        - Etcd
        provisionedMachineName: "ssh-controlplane-0"
        sshconfig:
          username: ubuntu
          host: 192.168.2.121
          port: 22
          publicKeys:
          - "ssh-rsa  pacific.cluster.cnct.io"
          secretName: master-private-key
    versions:
      kubelet: 1.10.1
      controlPlane: 1.10.1
    roles:
    - Master
- apiVersion: "cluster.k8s.io/v1alpha1"
  kind: Machine
  metadata:
    generateName: ssh-node-
    labels:
      set: node
  spec:
    providerConfig:
      value:
        apiVersion: "sshproviderconfig/v1alpha1"
        kind: "SSHMachineProviderConfig"
        roles:
        - Node
        provisionedMachineName: "ssh-node-0"
        sshconfig:
          username: ubuntu
          host: 192.168.2.187
          port: 22
          publicKeys:
          - "ssh-rsa  pacific.cluster.cnct.io"
          secretName: node-private-key
    versions:
      kubelet: 1.10.1
      controlPlane: 1.10.1
    roles:
    - Node